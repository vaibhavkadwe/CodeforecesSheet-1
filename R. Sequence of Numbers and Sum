#include<iostream>
using namespace std;
int main(){
    int a,b;
    int arr1[1000],arr2[1000];
    int i=0;
    while(true){
        cin>>a>>b;
        if(a<=0 || b<=0){
            break;
        }
        arr1[i]=a;
        arr2[i]=b;
        i++;
    }
    int x=0;
    int sum=0;
    while(i>0){
        sum =0;
        for(int j=min(arr1[x],arr2[x]);j<=max(arr1[x],arr2[x]);j++){
            cout<<j<<" ";
            sum=sum+j;
        }
        x++;
        i--;
        cout<<"sum ="<<sum<<endl;
    }
    return 0;
}