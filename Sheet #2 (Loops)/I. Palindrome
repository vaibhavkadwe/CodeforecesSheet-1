#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a,b;
    a=n;
    int rem=0;
    for(int i=0;n>0;i++){
        b=n%10;
        n=n/10;
        rem=rem*10+b;
    }
    cout<<rem<<endl;
    if(a==rem){
        cout<<"YES";
    }else{
        cout<<"NO";
    }
    return 0;
}