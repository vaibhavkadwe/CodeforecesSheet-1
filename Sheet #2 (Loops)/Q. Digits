#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int a,b;
    for(int i=0;i<n;i++){
        a=arr[i];
        if(arr[i]==0){
            cout<<0<<endl;
            continue;
        }
        while(a>0){
            b=a%10;
            a=a/10;
            cout<<b<<" ";
        }
        cout<<endl;
    }
    return 0;
}