#include<iostream>
using namespace std;
int main(){
    int t,a,b;
    cin>>t;
    int arr1[t],arr2[t];
    for(int i=0;i<t;i++){
        cin>>a>>b;
        arr1[i]=a;
        arr2[i]=b;
    }
    int x=0;
    int sum=0;
    while(t>0){
        sum=0;
        for(int i=min(arr1[x],arr2[x])+1;i<max(arr1[x],arr2[x]);i++){
            if(i%2==0){
                continue;
            }else{
                sum=sum+i;
            }
        }
        x++;
        t--;
        cout<<sum<<endl;
    }
    return 0;
}