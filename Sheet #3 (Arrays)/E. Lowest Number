#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    int* arr = new int[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int min=arr[0];
    int min1=0;
    for(int i=0;i<n;i++){
        if(arr[i]<min){
            min=arr[i];
            min1=i;
        }
    }
    cout<<min<<" "<<min1+1;
    delete[] arr;
    return 0;
}